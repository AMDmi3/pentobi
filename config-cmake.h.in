/* Define to 1 if you have the <unistd.h> header file. */
#cmakedefine01 HAVE_UNISTD_H

/* Define to 1 if you have the <sys/times.h> header file. */
#cmakedefine01 HAVE_SYS_TIMES_H

/* Version number of package */
#define VERSION "@PENTOBI_VERSION@"

#define DATADIR "@CMAKE_INSTALL_PREFIX@/share"

/* Floating type for Monte-Carlo tree search values (float|double) */
#define LIBBOARDGAME_MCTS_VALUE_TYPE @LIBBOARDGAME_MCTS_VALUE_TYPE@

/* Directory containing opening books.
   Path is absolute or relative to location of executable. */
#define LIBPENTOBI_MCTS_BOOK_DIR "@LIBPENTOBI_MCTS_BOOK_DIR@"

/** Location of the Pentobi user manual. */
#define PENTOBI_MANUAL_DIR "@PENTOBI_MANUAL_DIR@"

/** Location of the translations directory. */
#define PENTOBI_TRANSLATIONS "@PENTOBI_TRANSLATIONS@"

#ifdef _MSC_VER
// Don't report Visual C++ warning 4355 ('this' : used in base member
// initializer list) in default warning level 3. Storing a reference to
// 'this' is used at several places in the Pentobi code (e.g. constructors of
// thread functions). This is not a problem as long as 'this' is not used yet.
#pragma warning(4:4355)
#endif // _MSC_VER
